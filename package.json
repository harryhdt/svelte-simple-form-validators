{
  "name": "@svelte-simple-form/validators",
  "version": "0.0.1",
  "description": "Svelte Simple Form Validators",
  "homepage": "https://svelte-simple.harryhdt.dev/svelte-simple-form/validation",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harryhdt/svelte-simple-form-validators.git"
  },
  "keywords": [
    "svelte-simple-form",
    "svelte-simple-form-validators",
    "@svelte-simple-form",
    "@svelte-simple-form/validators",
    "standard-schema",
    "@svelte-simple-form/standard-schema",
    "@svelte-simple-form/validators/standard-schema"
  ],
  "author": "harryhdt",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
